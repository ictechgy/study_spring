package com.itbank.springStart;

public class AbstractGame implements Game {

	public abstract void remove();
	
	@Override
	public void move() {
		// TODO Auto-generated method stub
		System.out.println("게임캐릭터가 움직이다.");
	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		System.out.println("게임캐릭터가 멈추다.");
	}

	@Override
	public void fly() {
		// TODO Auto-generated method stub
		System.out.println("스프라이트가 날다.");
	}

}
